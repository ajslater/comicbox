{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/ajslater/comicbox/blob/main/schemas/v2.0/identifiers.schema.json",
  "title": "Comicbox Identifier",
  "type": "object",
  "patternProperties": {
    ".+": {
      "type": "object",
      "properties": {
        "id_key": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        }
      },
      "required": ["id_key"]
    }
  }
}
